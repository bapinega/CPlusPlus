#include <iostream>
using namespace std;

int main() {
    double a;
    cout << "Введите вещественное число a: ";
    cin >> a;
    
    double a2 = a * a; // a^2
    double a4 = a2 * a2; // a^4
    double a5 = a * a4; // a^5
    double a8 = a4 * a4; // a^8
    double a16 = a8 * a8; // a^16
    double a17 = a * a16; // a^17
    
    cout << "a^2 = " << a2 << endl;
    cout << "a^5 = " << a5 << endl;
    cout << "a^17 = " << a17 << endl;
    
    return 0;
}
