#include <iostream>

using namespace std;

int main() {
   int a, b, c;
   cout << "Введите значения переменных A, B, C: ";
   cin >> a >> b >> c;
   
   // Обмен значениями
   int temp = a;
   a = b;
   b = c;
   c = temp;
   
   // Вывод новых значений
   cout << "Новые значения переменных: " << endl;
   cout << "A = " << a << endl;
   cout << "B = " << b << endl;
   cout << "C = " << c << endl;
   
   return 0;
}
