#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Введите два целых числа: ";
    cin >> a >> b;
    
    if (a % 2 != 0 || b % 2 != 0) {
        cout << "Высказывание истинно" << endl;
    } else {
        cout << "Высказывание ложно" << endl;
    }
    
   return 0; 
}
